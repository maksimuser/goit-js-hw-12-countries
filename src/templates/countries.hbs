{{#each this}}
    <li class="list">
        <h1>{{name}}</h1>
        <img src="{{flag}}" alt="flag" width="340">
        <h3>capital:</h3>   
        <p>{{capital}}</p>
        <h3>population:</h3>  
        <p> {{population}}</p>
        <ol>
            <h3>languages:</h3>
            {{#each languages}}
            <li>
                {{name}}
            </li>
            {{/each}}
        </ol>
       
    </li>

{{/each}}